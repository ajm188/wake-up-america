{
  "manifest_version": 2,

  "name": "WAKE UP AMERICA",
  "description": "Satirical Chrome extension that transforms your browser text into a conservative website comment board ",
  "version": "0.1",

  "browser_action": {
    "default_icon": "assets/eagle.png",
    "default_title": "Wake Up America"
  },
  "permissions": [
    "activeTab",
	"storage"
  ],
  
  "content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}